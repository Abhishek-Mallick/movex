{
  "name": "movex-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/movex-react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
          "commands": [
              {
                  "command": "tsc --noEmit -p libs/movex-react/tsconfig.lib.json"
              }
          ]
      }
    },
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/movex-react",
        "tsConfig": "libs/movex-react/tsconfig.lib.json",
        "project": "libs/movex-react/package.json",
        "entryFile": "libs/movex-react/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/movex-react/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/movex-react/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/movex-react/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
